     "text": [
      "ChainMap({'x': 1, 'z': 3}, {'y': 2, 'z': 4})\n",
      "3\n"
     ]
    }
   ],
   "source": [
    "# 假设存在一个需求，先在a中寻找，后再在b中寻找\n",
    "from collections import ChainMap\n",
    "\n",
    "c = ChainMap(a,b)\n",
    "print(c)\n",
    "print(c['z'])"
   ]
  },
